{
    "name": "statelyworld/laravel-settings",
    "description": "A minimal Laravel package to manage key-value settings. A global application settings management package for Laravel, similar to WordPress options",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StatelyWorld\\LaravelSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StatelyWorld\\LaravelSettings\\SettingServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^9.0|^10.0|^11.0"
    }
}
